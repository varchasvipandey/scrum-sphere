{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-var": "error",
    "semi": "error",
    "quotes": [
      "error",
      "double"
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-multi-spaces": "error",
    "space-in-parens": "error",
    "no-multiple-empty-lines": "error",
    "prefer-const": "error",
    "no-unused-vars": "error"
  }
}